var returnResults = "TBD"

try {
	//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	
	// --- SET CURRENT APP VARIABLE NEEDED FOR DIALOGS &amp; StandardAdditions.osax ---
	var app = Application.currentApplication()
	app.includeStandardAdditions = true

	// --- SET KME APP VARIABLE NEEDED TO GET/SET KM VARIABLES ---
	var kme = Application("Keyboard Maestro Engine");
	
	var kmUserSupportFolder = app.pathTo('application support', { from: 'user domain' }) +
					"/Keyboard Maestro/";

	// --- GET THE MACRO CURRENTLY SELECTED IN THE KM EDITOR ---
	var macroUUID = getFirstSelectedMacroUUID();
	if (!macroUUID)
		throw Error("No macros selected");

	// --- GET THE PLIST OF ALL MACROS ---
	var plist = getMacrosPlist(kmUserSupportFolder);
	
	// --- GET THE DATA FOR THE SELECTED MACRO ---
	var macro = getMacro(plist, macroUUID);
	if (!macro)
		throw Error("Could not find Macro for UUID '" + macroUUID + "'");
	
	// --- SET KM VARIABLE FOR MACRO NAME ---
	kme.setvariable("MacroName", { to:  macro.Name });
	
	// --- GET DATA FOR THE FIRST ACTION, WHICH MUST BE A COMMENT ACTION ---
	var oComment = getInitialComment(macro);
	
	if (!oComment) {
	
		// --- CLEAR THE KM VARIABLES DUE TO NO COMMENT ACTION FOUND ---
		kme.setvariable("ActionName", { to:  '' });
		kme.setvariable("ActionTitle", { to:  '' });
		kme.setvariable("ActionText", { to:  '' });

		throw Error("FIRST Action in Macro MUST be a COMMENT Action.");
	}		// END No Comment Action

	// --- SET THE KM VARIABLES TO THE ACTION DATA ---
	kme.setvariable("ActionName", { to:  oComment.name });
	kme.setvariable("ActionTitle", { to:  oComment.title });
	kme.setvariable("ActionText", { to:  oComment.text });
	
	returnResults = "OK"
	
	//~~~~ END TRY ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

} catch (oError) {
	
	console.log(oError);
	
	if (oError.errorNumber === -128) {	// User Canceled
			returnResults =  "[USER_CANCELED]\n\n"			+ "SCRIPT: " + ptyScriptName + "   Ver: " + ptyScriptVer
	}
		else {		returnResults = "[ERROR]\n\n"			+ "SCRIPT: " + ptyScriptName + "   Ver: " + ptyScriptVer + "\n"			+ "Error Number: " + oError.errorNumber + "\n"			+ oError.message	} // END if/else
	
} // END catch
//~~~~ END TRY/CATCH BLOCK ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

return returnResults

//~~~~~~~~~~~~~~~~~~~~~~~~~~~~ END OF MAIN SCRIPT ~~~~~~~~~~~~~~~~~

//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
function getFirstSelectedMacroUUID() {
//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

	// --- NEED REF TO KM EDITOR APP FOR THIS ---
	var selections = Application("Keyboard Maestro").selectedmacros();
	return selections.length &gt; 0 ? selections[0] : "";
}

//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
function getMacrosPlist(pKMFolder) {
//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	return readPlistBinaryFile(pKMFolder + "Keyboard Maestro Macros.plist")
}
		
//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
function readPlistBinaryFile(pFilePath) {
//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

			var data = $.NSData.dataWithContentsOfFile(pFilePath);
			return ObjC.deepUnwrap(
				$.NSPropertyListSerialization.propertyListWithDataOptionsFormatError(
					data, $.NSPropertyListBinaryFormat_v1_0, 0, null));
}

//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
function getMacro(plist, macroUUID) {
//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

	for (var i = 0; i &lt; plist.MacroGroups.length; i++) {
		var group = plist.MacroGroups[i];
		if (group.Macros) {
			for (var j = 0; j &lt; group.Macros.length; j++) {
				var macro = group.Macros[j];
				if (macro.UID == macroUUID)
					return macro;
					
			}		// END for j
		}		// END if
	}		// END for i
	
	return "";
	
}		// END function getMacro

//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
function getInitialComment(macro) {
//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

		var results = [];
		if (!macro.Actions || macro.Actions.length === 0)
			return null;

		var action = macro.Actions[0];
		if (action.MacroActionType !== "Comment")
			return null;
			
		return {
			name: action.ActionName || action.Title || "",
			title: action.Title || "",
			text: action.Text || ""
		};
	}



})();	// ~~~ function run() is automatically executed when this script is executed ~~~

