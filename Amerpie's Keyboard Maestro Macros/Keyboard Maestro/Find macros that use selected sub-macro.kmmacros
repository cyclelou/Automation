&lt;html&gt;

&lt;head&gt;
    &lt;title&gt;Matches&lt;/title&gt;

    &lt;style type="text/css"&gt;
        body {
            font-family: verdana;
            font-size: 16px;
            font-weight: 400;
            color: #fff;
            background: #353a40;
        }

        .heading {
            text-align: center;
            margin-bottom: 15px;
        }

        table {
            border-collapse: separate;
            background: #fff;
            border-radius: 5px;
            margin: auto;
        }

        thead th {
            font-size: 16px;
            font-weight: 400;
            color: #fff;
            text-shadow: 1px 1px 0px rgba(0, 0, 0, 0.5);
            text-align: left;
            padding: 10px;
            background-size: 100%;
            background-image: linear-gradient(#646f7f, #4a5564);
            border-top: 1px solid #858d99;
        }

        thead th:first-child {
            border-top-left-radius: 5px;
        }

        thead th:last-child {
            border-top-right-radius: 5px;
        }

        tbody tr td {
            font-weight: 400;
            color: black;
            font-size: 13px;
            padding: 5px 5px 5px 5px;
            border-bottom: 1px solid #e0e0e0;
        }

        tbody tr:nth-child(2n) {
            background: #f0f3f5;
        }

        tbody tr:last-child td {
            border-bottom: none;
        }

        tbody tr:last-child td:first-child {
            border-bottom-left-radius: 5px;
        }

        tbody tr:last-child td:last-child {
            border-bottom-right-radius: 5px;
        }

        .myButton {
            box-shadow: 0px 0px 0px 2px #9fb4f2;
            background: linear-gradient(to bottom, #7892c2 5%, #476e9e 100%);
            background-color: #7892c2;
            border-radius: 10px;
            border: 1px solid #4e6096;
            display: inline-block;
            cursor: pointer;
            color: #ffffff;
            font-family: verdana;
            font-size: 14px;
            padding: 4px 30px;
            text-decoration: none;
            text-shadow: 0px 1px 0px #283966;
        }

        .myButton:hover {
            background: linear-gradient(to bottom, #476e9e 5%, #7892c2 100%);
            background-color: #476e9e;
        }

        .myButton:active {
            position: relative;
            top: 1px;
        }
    &lt;/style&gt;

    &lt;script type="text/javascript"&gt;
        //
        // Add a table dynamically, at runtime. Pass the "id" of the div that
        // the table should be put in, the class name fo the table (so you can
        // style it with css), and the data to put in the table.
        //
        // For "rowData", it's an array of data for each row. See the comments
        // for the "contents" parameter of the "addRow" function for how to
        // define this data.
        function addTable(divID, tableClassName, rowData) {

            // Add a cell.
            //
            // "uri" is optional. If present, the cell's content is an href
            // with "value" as the display value, and "uri" as the link.
            // Otherwise it's a regular text cell, with "value" as the content.
            function addCell(tr, cellType, value, uri) {
                var cell = document.createElement(cellType);
                if (uri) {
                    var a = document.createElement("a");
                    a.setAttribute("href", uri);
                    a.innerHTML = value;
                    cell.appendChild(a);
                } else {
                    cell.appendChild(document.createTextNode(value));
                }
                tr.appendChild(cell);
            }

            // Add a row.
            //
            // "cellType" is "th" or "td".
            //
            // "contents" is an array of cell values. For each cell, you can
            // indlude one value as the text, like this:
            //      row = [cell one, cell two], etc.
            // or you can specify an href, with a cell's content being a an
            // array of 2 items: the display value and the URI. In this example,
            // the second cell is an href:
            //      row = [cell one, [cell two, http://...]]
            // If this format is used, you can still pass an empty value as the
            // URI if needed - in this case it will be ignored and a standard
            // text cell will be added. ("empty" means undefined, null, or "".)
            // You can intermix standard cell contents and hrefs however you want.
            function addRow(tableBody, cellType, contents) {
                var tr = document.createElement("tr");
                contents.forEach(function(content) {
                    var value, uri;
                    if (content) {
                        if (Array.isArray(content)) {
                            value = content[0];
                            uri = content[1]; // ok if there is no [1]
                        } else {
                            value = content;
                        }
                    }
                    addCell(tr, cellType, value, uri);
                });
                tableBody.appendChild(tr);
            }

            //------------------------------------------------------------------
            // addTable(macros)
            var div = document.getElementById(divID);
            var table = document.createElement('table');
            table.className = tableClassName;

            var thead = document.createElement('thead');
            table.appendChild(thead);
            addRow(thead, "th", ["Group", "Macro"]);

            var tableBody = document.createElement('tbody');

            table.appendChild(tableBody);
            rowData.forEach(function(row) {
                addRow(tableBody, "td", row);
            });
            div.appendChild(table)
        } // addTable(macros)

        function makeKMURI(uuid) {
            return 'keyboardmaestro://m=' + uuid;
        }

        function KMDidShowWindow() {
            try {
                var macroName = window.KeyboardMaestro.GetVariable('fmumTargetMacroName');
                var macros = JSON.parse(window.KeyboardMaestro.GetVariable('fmumMatches'));
                heading.innerText = 'Macros that use Sub-Macro\n"' + macroName + '"';

                var rowData = macros.map(function(macro) {
                    return [
                        [macro.groupName, makeKMURI(macro.groupUUID)],
                        [macro.macroName, makeKMURI(macro.macroUUID)]
                    ];
                });
                addTable("divTable", "gridtable", rowData);
            } catch (e) {
                document.write("Error in KMDidShowWindow(): " + e + "&lt;br&gt;");
            }
        }

        function KMWindow() {
            try {
                var bodyWidth = window.KeyboardMaestro.GetVariable('fmumBodyWidth');
                var bodyHeight = window.KeyboardMaestro.GetVariable('fmumBodyHeight');
                document.body.style.width = bodyWidth + 'px';
                document.body.style.height = bodyHeight + 'px';
                return "fmumWindowWidth, fmumWindowHeight";
            } catch (e) {
                document.write("Error in KMWindow(): " + e + "&lt;br&gt;");
            }
        }
    &lt;/script&gt;
&lt;/head&gt;

&lt;body&gt;
    &lt;div id="heading" class="heading"&gt;
