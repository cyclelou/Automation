    &lt;div id="divButton" style="text-align:center; margin-top: 10px;"&gt;
        &lt;input type="button" class="myButton" value="Close" onclick="window.KeyboardMaestro.Submit('OK')"&gt;
    &lt;/div&gt;
&lt;/body&gt;

